{
    "name": "Progenitor",
    "description": "Progenitor is a light-weight installation profile that provides a basic starting place for Phase2 projects.",
    "require-dev": {
        "phpmd/phpmd": "1.4.*",
        "pdepend/pdepend": "1.1.*",
        "squizlabs/php_codesniffer": "1.*",
        "drupal/coder": "7.2.2"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/coder",
                "version": "7.2.2",
                "dist": {
                    "url": "http://ftp.drupal.org/files/projects/coder-7.x-2.2.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "http://git.drupal.org/project/coder.git",
                    "type": "git",
                    "reference": "tags/7.x-2.2"
                }
            }
        }
    ]
}
